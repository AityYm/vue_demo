{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": ["transform-vue-jsx", "transform-runtime",["component", [// component专门按需打包插件 ---> 针对mint-ui
    {
      "libraryName": "mint-ui",
      "style": true
    }
  ]]]
}
